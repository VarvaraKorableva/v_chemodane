{
    "compilerOptions": {
        "target": "ESNext",
        "module": "esnext",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "allowJs": false,
        "checkJs": false,
        "isolatedModules": true,
        "resolveJsonModule": true,
        "strict": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noEmit": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "baseUrl": ".",
    },
    "include": [
        // "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}